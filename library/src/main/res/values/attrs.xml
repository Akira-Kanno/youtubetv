<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="YoutubeTvView">
        <attr name="videoId" format="string" />
        <attr name="videoQuality" format="enum">
            <enum name="small" value="0" />
            <enum name="medium" value="1" />
            <enum name="large" value="2" />
            <enum name="hd720" value="3" />
            <enum name="hd1080" value="4" />
            <enum name="highres" value="5" />
            <enum name="auto" value="6" />
        </attr>
        <attr name="showRelatedVideos" format="boolean" />
        <attr name="showVideoInfo" format="boolean" />
        <attr name="showControls" format="enum">
            <enum name="auto" value="1" />
            <enum name="always" value="2" />
            <enum name="none" value="0" />
        </attr>
        <attr name="autoHide" format="enum">
            <enum name="auto" value="0" />
            <enum name="displayAuto" value="1" />
            <enum name="alwaysVisible" value="2" />
        </attr>
        <attr name="closedCaptions" format="boolean" />
        <attr name="videoAnnotation" format="boolean" />
        <attr name="debug" format="boolean" />
        <attr name="loadingBackgroundColor" format="integer" />
        <attr name="autoplay" format="boolean" />
        <attr name="userAgentString" format="enum">
            <enum name="auto" value="0" />
            <enum name="iphone" value="1" />
            <enum name="ipad" value="2" />
            <enum name="desktop" value="3" />
            <enum name="mobileDesktop" value="4" />
        </attr>
        <attr name="showBorder" format="boolean" />
        <attr name="borderWidth" format="integer" />
        <attr name="borderColor" format="color" />
        <attr name="thumbnailQuality" format="enum">
            <enum name="auto" value="0" />
            <enum name="hqdefault" value="1" />
            <enum name="mqdefault" value="2" />
            <enum name="sddefault" value="3" />
            <enum name="maxresdefault" value="4" />
        </attr>
    </declare-styleable>
</resources>
